{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "lib": [ "es2015", "dom" ],
    "removeComments": false,
    "noImplicitAny": false,
    "suppressImplicitAnyIndexErrors": true

  },
  "compileOnSave": true,
  "exclude": [
    "node_modules/*",
    "wwwroot/npm-lib/*",
    "bin",
    "**/*-aot.ts"
  ]
}
